<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_actividades2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="me.health.actividades2">

    <TextView
        android:text="Activities"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:id="@+id/textView7"
        android:textSize="34sp"
        android:textColor="@android:color/holo_blue_bright"
        android:layout_marginTop="54dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/floatingGuru"
        android:id="@+id/imageButton4"
        android:background="@android:color/background_light"
        android:layout_marginBottom="22dp"
        android:layout_alignBottom="@+id/imageButton3"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="71dp" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/leaderboard"
        android:layout_marginTop="32dp"
        android:id="@+id/imageButton3"
        android:background="@android:color/background_light"
        android:layout_below="@+id/imageButton"
        android:layout_alignStart="@+id/imageButton" />

    <TextView
        android:text="Contact Sensei"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:textSize="18sp"
        android:textColor="@android:color/black"
        android:layout_alignBottom="@+id/imageButton3"
        android:layout_alignEnd="@+id/textView9" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/goodinfo"
        android:id="@+id/imageButton5"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:background="@android:color/background_light" />

    <TextView
        android:text="Goals"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        android:textSize="18sp"
        android:textColor="@android:color/black"
        android:layout_alignBottom="@+id/imageButton3"
        android:layout_alignEnd="@+id/imageButton3"
        android:layout_marginEnd="24dp" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/RollerSkating"
        android:id="@+id/imageButton"
        android:background="@android:color/background_light"
        android:layout_marginEnd="27dp"
        android:layout_alignBottom="@+id/textView13"
        android:layout_toStartOf="@+id/textView11" />

    <TextView
        android:text="Skating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        android:textSize="18sp"
        android:textColor="@android:color/black"
        android:layout_below="@+id/textView7"
        android:layout_alignEnd="@+id/textView14"
        android:layout_marginTop="14dp" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/Weightlifting"
        android:id="@+id/imageButton2"
        android:background="@android:color/background_light"
        android:layout_marginEnd="40dp"
        android:layout_alignTop="@+id/imageButton"
        android:layout_toStartOf="@+id/imageButton5" />

    <TextView
        android:text="Weight Lifting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:textSize="18sp"
        android:textColor="@android:color/black"
        android:layout_alignBottom="@+id/imageButton2"
        android:layout_alignStart="@+id/imageButton2" />
</RelativeLayout>
